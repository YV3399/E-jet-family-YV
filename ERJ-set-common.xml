<?xml version="1.0" encoding="UTF-8" ?>

<PropertyList>

	<sim>
		<author>Gabriel Hernandez (YV3399), D-ECHO</author>
                <status>Version 0.6</status>
		<flight-model>jsb</flight-model>

		<model>
			<livery>
				<file type="string"/>
			</livery>

			<E-jet-family>
				<heading-bug-pfd-deg type="double">0</heading-bug-pfd-deg>
				<heading-bug-deg type="double">0</heading-bug-deg>
				<nav1-bug-deg type="double">0</nav1-bug-deg>
				<nav2-bug-deg type="double">0</nav2-bug-deg>
				<wp-bug-deg type="double">0</wp-bug-deg>
				<ias-bug-kt-norm type="double">0</ias-bug-kt-norm>
				<mach-bug-kt-norm type="double">0</mach-bug-kt-norm>
			</E-jet-family>

			<pushback>
				<kp type="double">300</kp>
				<ki type="double">0</ki>
				<kd type="double">0</kd>
				<position-norm type="double">0</position-norm>
			</pushback>

			<door n="0">
				<position-y-m>-1.489</position-y-m>
				<position-z-m>0.034</position-z-m>
				<jetway-hood-deg>8</jetway-hood-deg>
			</door>

			<rendering>
				<reflection-enabled type="int">1</reflection-enabled>
				<reflection-type type="int">2</reflection-type>
				<reflection-dynamic type="int">1</reflection-dynamic>
				<reflection-correction type="float">0.1</reflection-correction>
				<reflection-fresnel type="float">0.5</reflection-fresnel>
				<reflection-fresnel-factor type="float">1.0</reflection-fresnel-factor>
				<reflection-rainbow type="float">0.1</reflection-rainbow>
				<reflection-noise type="float">0.05</reflection-noise>
				<ambient-correction type="float">0.15</ambient-correction>
			</rendering>
		</model>

		<flaps>
			<setting>0</setting>
			<setting>0.1</setting>
			<setting>0.2</setting>
			<setting>0.3</setting>
			<setting>0.4</setting>
			<setting>0.5</setting>
			<setting>0.6</setting>
		</flaps>

		<startup>
			<splash-texture>Aircraft/E-jet-family/Splash/splash.png</splash-texture>
		</startup>

		<previews>
			<preview>
				<type>exterior</type>
				<splash type="bool">true</splash>
				<path>Splash/splash.png</path>
			</preview>
		</previews>

		<systems>
			<autopilot>
				<name>Autopilot</name>
				<path>Aircraft/E-jet-family/Systems/it-autoflight.xml</path>
			</autopilot>
			<autopilot>
				<name>Autothrust</name>
				<path>Aircraft/E-jet-family/Systems/it-autothrust.xml</path>
			</autopilot>
			<autopilot>
				<name>Autobrake</name>
				<path>Aircraft/E-jet-family/Systems/ERJ-autobrake.xml</path>
			</autopilot>
			<autopilot>
				<name>Gear AGL Altitude</name>
				<path>Aircraft/E-jet-family/Systems/gear-agl-ft.xml</path>
			</autopilot>
			<autopilot>
				<name>Spoilers</name>
				<path>Aircraft/E-jet-family/Systems/ERJ-spoilers.xml</path>
			</autopilot>
			<property-rule n="100">
				<path>Aircraft/E-jet-family/Systems/pfd.xml</path>
			</property-rule>
			<electrical><path></path></electrical><!--Disable XML electrical system-->
		</systems>

		<chase-distance-m>-100</chase-distance-m>

		<sound>
			<path>Aircraft/E-jet-family/ERJ-sound.xml</path>
		</sound>

		<instrumentation>
			<path>Aircraft/E-jet-family/Systems/instrumentation.xml</path>
		</instrumentation>

		<alarms>
			<master-caution type="bool">false</master-caution>
		</alarms>

		<rendering>
			<redout>
				<enabled type="bool">true</enabled>
				<parameters>
					<blackout-complete-g type="double">7</blackout-complete-g>
					<blackout-onset-g type="double">4.5</blackout-onset-g>
					<redout-complete-g type="double">-4.5</redout-complete-g>
					<redout-onset-g type="double">-2</redout-onset-g>
				</parameters>
			</redout>

			<camera-group>
				<znear type="double">0.1</znear>
			</camera-group>
		</rendering>

		<view n="0">
			<name>Pilot View</name>
			<internal archive="y">true</internal>
			<config>
				<x-offset-m archive="y">-0.53</x-offset-m>
				<y-offset-m archive="y">1.43</y-offset-m>
				<pitch-offset-deg archive="y">-8.0</pitch-offset-deg>
				<default-field-of-view-deg type="double">70.0</default-field-of-view-deg>
			</config>
		</view>

		<view n="100">
			<name>Gear View</name>
			<type>lookfrom</type>
			<internal archive="y">false</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">0</x-offset-m>
				<y-offset-m archive="y">-1.63</y-offset-m>
				<z-offset-m archive="y">0.99</z-offset-m>
				<pitch-offset-deg archive="y">0</pitch-offset-deg>
			</config>
		</view>

		<view n="101">
			<name>Wing View</name>
			<type>lookfrom</type>
			<internal archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<from-model-idx type="int">0</from-model-idx>
				<ground-level-nearplane-m type="double">0.01</ground-level-nearplane-m>
				<default-field-of-view-deg type="double">70</default-field-of-view-deg>
				<pitch-offset-deg>-8.0</pitch-offset-deg>
				<heading-offset-deg type="double">90</heading-offset-deg>
				<!-- parallel to the panel -->
				<x-offset-m archive="y">-1.84 </x-offset-m>
				<!--Left/Right-->
				<y-offset-m archive="y"> 0.99 </y-offset-m>
				<!--Up/Down-->
				<z-offset-m archive="y"> 0.23 </z-offset-m>
				<!--Fdw/Back-->
			</config>
		</view>

		<view n="102">
			<name>Tail view</name>
			<type>lookfrom</type>
			<internal type="bool">false</internal>
			<config>
				<from-model type="bool">true</from-model>
				<from-model-idx type="int">0</from-model-idx>
				<ground-level-nearplane-m type="double">6f</ground-level-nearplane-m>
				<x-offset-m type="double">0.00</x-offset-m>
				<y-offset-m type="double">7.64</y-offset-m>
				<z-offset-m type="double">13.14</z-offset-m>
				<pitch-offset-deg>-10</pitch-offset-deg>
				<default-field-of-view-deg type="double">65.0</default-field-of-view-deg>
			</config>
			<dynamic>
				<enabled type="bool">false</enabled>
			</dynamic>
		</view>

		<view n="103">
			<name>Copilot View</name>
			<type>lookfrom</type>
			<internal archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">0.53</x-offset-m>
				<y-offset-m archive="y">1.43</y-offset-m>
				<pitch-offset-deg archive="y">-8.0</pitch-offset-deg>
			</config>
		</view>

		<view n="104">
			<type>lookfrom</type>
			<internal archive="y">true</internal>
			<config>
				<from-model type="bool">true</from-model>
				<x-offset-m archive="y">-1.09</x-offset-m>
				<y-offset-m archive="y">1.28</y-offset-m>
				<z-offset-m archive="y">0</z-offset-m>
			</config>
		</view>

		<menubar>
			<default>
				<menu n="100">
					<label>E-jet-family</label>
					<enabled type="bool">true</enabled>
					<item>
						<name>autostart</name>
						<label>Autostart</label>
						<binding>
							<command>property-toggle</command>
							<property>sim/model/start-idling</property>
						</binding>
					</item>
					<item>
						<label>Select Livery</label>
						<binding>
							<command>nasal</command>
							<script>
								aircraft.livery.dialog.open()
							</script>
						</binding>
					</item>
					<item>
						<label>Airport Operations</label>
						<binding>
							<command>dialog-show</command>
							<dialog-name>Airport operations</dialog-name>
						</binding>
					</item>
					<item>
						<label>Announcements</label>
						<binding>
							<command>dialog-show</command>
							<dialog-name>Cabin Announcements</dialog-name>
						</binding>
					</item>
					<item>
						<name>pushback</name>
						<label>Pushback</label>
						<binding>
							<command>dialog-show</command>
							<dialog-name>pushback</dialog-name>
						</binding>
					</item>
					<item>
						<name>lights</name>
						<label>Aircraft Lights</label>
						<binding>
							<command>dialog-show</command>
							<dialog-name>lights</dialog-name>
						</binding>
					</item>
					<item>
						<name>tiller</name>
						<label>Tiller Steering</label>
						<binding>
							<command>dialog-show</command>
							<dialog-name>tiller-dlg</dialog-name>
						</binding>
					</item>
				</menu>
			</default>
		</menubar>

		<help>
			<title>E-jet-family</title>
			<key>
				<name>Delete</name>
				<desc>toggle thrust reversers</desc>
			</key>
			<key>
				<name>Ctrl-z</name>
				<desc>toggle master autopilot switch</desc>
			</key>
			<key>
				<name>Ctrl-y</name>
				<desc>toggle autothrottle switch</desc>
			</key>
			<key>
				<name>Ctrl-b</name>
				<desc>cycle speedbrake lever setting</desc>
			</key>
			<key>
				<name>Ctrl-k</name>
				<desc>arm/disarm ground spoilers</desc>
			</key>
			<key>
				<name>=</name>
				<desc>cycle autobrake setting</desc>
			</key>
			<key>
				<name>w</name>
				<desc>walk forward toward view direction</desc>
			</key>
			<key>
				<name>W</name>
				<desc>run forward toward view direction</desc>
			</key>
			<key>
				<name>s</name>
				<desc>walk backward from view direction</desc>
			</key>
		</help>

		<multiplay>
			<generic>
				<int n="0" alias="/systems/electrical/outputs/strobe"/>
				<int n="1" alias="/systems/electrical/outputs/beacon"/>
				<int n="2" alias="/systems/electrical/outputs/taxi-lights"/>
				<int n="3" alias="/systems/electrical/outputs/logo-lights"/>
				<int n="4" alias="/systems/electrical/outputs/landing-light[0]"/>
				<int n="5" alias="/systems/electrical/outputs/landing-light[1]"/>
				<int n="6" alias="/systems/electrical/outputs/landing-light[2]"/>
				<int n="7" alias="/systems/electrical/outputs/nav-lights"/>
				<float n="0" alias="/gear/gear[0]/compression-m"/>
				<float n="1" alias="/gear/gear[1]/compression-m"/>
				<float n="2" alias="/gear/gear[2]/compression-m"/>
				<float n="3" alias="/gear/gear[1]/compression-m"/>
				<float n="4" alias="/gear/gear[2]/compression-m"/>
				<float n="5" alias="/surface-positions/speedbrake-norm"/>
				<float n="6" alias="/surface-positions/reverser-norm"/>
				<string alias="/sim/multiplay/callsign"/>
			</generic>
		</multiplay>
		
		
		
	</sim>

	
	<it-autoflight>
		<config>
			<roll>
				<umin>-0.50</umin>
				<umax>0.50</umax>
				<kp-low>0.05</kp-low>
				<kp-high>0.09</kp-high>
				<ti>10.0</ti>
				<td>0.00001</td>
			</roll>
			<pitch>
				<umin>-0.50</umin>
				<umax>0.50</umax>
				<kp-low>-0.08</kp-low>
				<kp-high>-0.08</kp-high>
				<ti>10.0</ti>
				<td>0.0001</td>
			</pitch>
			<cmd>
                <hdg>1.0</hdg>
                <vorloc>1.0</vorloc>
                <alt-gain>-5</alt-gain>
			</cmd>
			<autoland>
				<pitch-kp>0.03</pitch-kp>
				<yaw-kp>-0.1</yaw-kp>
			</autoland>
			<rollout>
                <roll-kp>0.05</roll-kp>
				<pitch-defl>0.15</pitch-defl>
				<yaw-kp>-0.1</yaw-kp>
			</rollout>
		</config>
		<settings>
			<auto-bank-max-deg>35</auto-bank-max-deg> <!-- Maximum Auto Bank Limit -->
			<land-enable>1</land-enable> <!-- Enable/Disable Autoland -->
			<land-flap>0.5</land-flap> <!-- Minimum Flap used for Landing -->
			<retard-ft>50</retard-ft> <!-- Enable Thrust Retard -->
			<retard-enable>1</retard-enable> <!-- AGL to Thrust Retard -->
			<togaspd>165</togaspd> <!-- V2 + 10kts -->
			<lat-agl-ft>200</lat-agl-ft> <!-- Set to 999999 if you do not want T/O to change automatically to HDG, or LNAV -->
			<reduc-agl-ft>1300</reduc-agl-ft> <!-- Set to 999999 if you do not want TOGA to change automatically to FLCH -->
			<autoland-without-ap>1</autoland-without-ap> <!-- Engage LAND and FLARE Modes even if the AP is off -->
		</settings>
	</it-autoflight>
		
	
	<consumables>
