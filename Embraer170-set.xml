<?xml version="1.0" encoding="UTF-8" ?>
<PropertyList include="common.xml">
    <sim>
        <description>Embraer E-Jet 170 (170-100)</description>
        <aero>FDM/E170</aero>
        <engine>GE_CF34-8E</engine>

        <model>
            <path>Aircraft/E-jet-family/Models/E170.xml</path>
            <door n="0">
                <position-x-m>-9.443</position-x-m>
            </door>

            <chock-offsets>
                <front>5.2</front>
                <main>-5.7</main>
            </chock-offsets>
        </model>

        <rating>
            <FDM type="int">3</FDM>
            <systems type="int">3</systems>
            <cockpit type="int">4</cockpit>
            <model type="int">5</model>
        </rating>

        <view n="0">
            <config>
                <z-offset-m archive="y">-11.57</z-offset-m>
            </config>
        </view>
        <view n="201">
            <config>
                <z-offset-m archive="y">-11.57</z-offset-m>
            </config>
        </view>
        <view n="200">
            <config>
                <z-offset-m archive="y">-11.57</z-offset-m>
            </config>
        </view>
        <view n="199">
            <config>
                <z-offset-m archive="y">-12.2</z-offset-m>
            </config>
        </view>
        <view n="198">
            <config>
                <z-offset-m archive="y">-11.57</z-offset-m>
            </config>
        </view>
        <view n="197">
            <config>
                <z-offset-m archive="y">-11.57</z-offset-m>
            </config>
        </view>
        <view n="103">
            <config>
                <z-offset-m archive="y">-11.57</z-offset-m>
            </config>
        </view>
        <view n="104">
            <name>Passenger View (Economy Class)</name>
            <config>
                <walkable type="bool">true</walkable>
            </config>
        </view>

        <previews>
            <preview>
                <type>exterior</type>
                <path>Splash/E170/440.jpg</path>
                <splash type="bool">true</splash>
            </preview>
            <preview>
                <type>exterior</type>
                <path>Splash/E170/939.jpg</path>
                <splash type="bool">true</splash>
            </preview>
            <preview>
                <type>exterior</type>
                <path>Splash/E170/119.jpg</path>
                <splash type="bool">true</splash>
            </preview>
            <preview>
                <type>exterior</type>
                <path>Splash/E170/756.jpg</path>
                <splash type="bool">true</splash>
            </preview>
            <preview>
                <type>exterior</type>
                <path>Splash/E170/024.jpg</path>
                <splash type="bool">true</splash>
            </preview>
            <preview>
                <type>exterior</type>
                <path>Splash/E170/651.jpg</path>
                <splash type="bool">true</splash>
            </preview>
        </previews>
    </sim>


    <payload>
        <weight n="1">
            <name>Passengers Front</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[1]"/>
            <min-lb type="double">0</min-lb>
            <max-lb type="double">9750</max-lb>
            <unit-lb type="double">250</unit-lb>
            <unit-count type="int">0</unit-count>
        </weight>
        <weight n="2">
            <name>Passengers Back</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[2]"/>
            <min-lb type="double">0</min-lb>
            <max-lb type="double">9750</max-lb>
            <unit-lb type="double">250</unit-lb>
            <unit-count type="int">0</unit-count>
        </weight>
        <weight n="3">
            <name>Cargo Front</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[3]"/>
            <min-lb type="double">0</min-lb>
            <max-lb type="double">4079</max-lb>
        </weight>
        <weight n="4">
            <name>Cargo Back</name>
            <weight-lb alias="/fdm/jsbsim/inertia/pointmass-weight-lbs[4]"/>
            <min-lb type="double">0</min-lb>
            <max-lb type="double">3175</max-lb>
        </weight>
    </payload>

    <limits>
        <!-- values are estimations -->

        <vne>370</vne>

        <max-positive-g>4.4</max-positive-g>
        <max-negative-g>-1.76</max-negative-g>

        <max-flap-extension-speed n="0">
            <flaps>0.10</flaps>
            <speed>265</speed>
        </max-flap-extension-speed>
        <max-flap-extension-speed n="1">
            <flaps>0.20</flaps>
            <speed>240</speed>
        </max-flap-extension-speed>
        <max-flap-extension-speed n="2">
            <flaps>0.03</flaps>
            <speed>225</speed>
        </max-flap-extension-speed>
        <max-flap-extension-speed n="3">
            <flaps>0.40</flaps>
            <speed>210</speed>
        </max-flap-extension-speed>

        <max-gear-extension-speed>220</max-gear-extension-speed>

        <mass-and-balance>
            <maximum-takeoff-mass-lbs>85098</maximum-takeoff-mass-lbs>
            <maximum-landing-mass-lbs>73414</maximum-landing-mass-lbs>
            <maximum-payload-lbs>21515</maximum-payload-lbs>
            <maximum-ramp-mass-lbs>88703</maximum-ramp-mass-lbs>
            <maximum-zero-fuel-mass-lbs>68123</maximum-zero-fuel-mass-lbs>
        </mass-and-balance>
    </limits>

    <instrumentation>
        <mcdu>
            <ident>
                <model>E170-100</model>
                <engines>GE_CF34-8E</engines>
            </ident>
        </mcdu>
    </instrumentation>

    <fadec>
        <configuration>
            <idle-n1>25.0</idle-n1>
            <max-n1>99.5</max-n1>
        </configuration>
    </fadec>

    <trs>
        <thrust n="0">
            <to1-nominal type="double">14500</to1-nominal>
            <to2-nominal type="double">13500</to2-nominal>
            <to3-nominal type="double">12200</to3-nominal>
        </thrust>
    </trs>

</PropertyList>
