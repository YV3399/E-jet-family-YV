<?xml version="1.0"?>
<PropertyList>
    <filter>
        <type>noise-spike</type>
        <input>/instrumentation/clipboard/hovering</input>
        <output>/instrumentation/clipboard/gui-illum</output>
        <max-rate-of-change>
            <condition>
                <greater-than>
                    <property>/instrumentation/clipboard/hovering</property>
                    <property>/instrumentation/clipboard/gui-illum</property>
                </greater-than>
            </condition>
            <value>8</value>
        </max-rate-of-change>
        <max-rate-of-change>0.2</max-rate-of-change>
    </filter>
    <filter>
        <type>noise-spike</type>
        <output>/instrumentation/clipboard/hovering</output>
        <input>
            <value>0</value>
        </input>
        <max-rate-of-change>0.01</max-rate-of-change>
    </filter>
</PropertyList>
