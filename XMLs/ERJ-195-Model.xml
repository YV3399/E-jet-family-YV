<!-- Landing Gear Animations -->

<!-- Sub Definitions -->
    <animation>
        <name>NoseGear</name>
        <object-name>gear.n</object-name>
        <object-name>tire.n</object-name>
    </animation>

    <animation>
        <name>LeftGear</name>
        <object-name>gear.l</object-name>
        <object-name>tire.l</object-name>
    </animation>

    <animation>
        <name>RightGear</name>
        <object-name>gear.r</object-name>
        <object-name>tire.r</object-name>
    </animation>

<!-- Nose Gear -->
    <animation>
        <type>rotate</type>
        <object-name>NoseGear</object-name>
        <property>controls/flight/rudder</property>
        <factor>60</factor>
        <center>
            <x-m>-13.82</x-m>
            <y-m>0.0</y-m>
            <z-m>-1.91</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>-1.0</z>
        </axis>
    </animation>

    <animation>
        <type>rotate</type>
        <object-name>NoseGear</object-name>
        <property>gear/gear[0]/position-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>115</dep>
            </entry>
            <entry>
                <ind>0.2</ind>
                <dep>115</dep>
            </entry>
            <entry>
                <ind>1.0</ind>
                <dep>0</dep>
            </entry>
        </interpolation>
        <center>
            <x-m>-13.60</x-m>
            <y-m>0.0</y-m>
            <z-m>-0.84</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>translate</type>
         <object-name>tire.n</object-name>
        <property>gear/gear[0]/compression-norm</property>
        <interpolation>
            <entry><ind>0.00</ind><dep>0</dep></entry>
            <entry><ind>3</ind><dep>-0.2</dep></entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
    </animation>

<!-- Left Gear -->
    <animation>
        <type>rotate</type>
        <object-name>LeftGear</object-name>
        <property>gear/gear[1]/position-norm</property>
        <interpolation>
            <entry><ind>0</ind><dep>75</dep></entry>
            <entry><ind>0.2</ind><dep>75</dep></entry>
            <entry><ind>0.8</ind><dep>0</dep></entry>
            <entry><ind>1</ind><dep>0</dep></entry>
        </interpolation>
        <center>
            <y-m>-2.53</y-m>
